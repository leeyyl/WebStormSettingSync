      <register name="5" type="4">
        <text>const start = ctx.query.start
    const end = ctx.query.end
    let day = moment(start)
    const result = []
    while (day.isSameOrBefore(moment(end))) {
      const record = { day: day }
      record.first = await report.firstDayNum(ctx, day, 1)
      const logins = await report.dailyLogin(ctx, day)
      for (const item of logins) {
        const key = moment(item.day).diff(day, 'days') + 1
        record[key] = (item.sum / record.first).toFixed(2)
      }
      result.push(record)
      day = day.add(1, 'days')
    }
    ctx.json(result)</text>
